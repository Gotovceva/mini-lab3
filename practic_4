# 1 задание, 5-фя функция(f(x) = a x^2 + b
import matplotlib.pyplot as plt
import numpy as np
a = float(input("Введите коэффициент a:"))
b = float(input("Введите коэффициент b:"))
x_min = float(input("Введите минимальное значение x: "))
x_max = float(input("Введите максимальное значение y: "))
x = np.linspace(x_min, x_max, 200)
y = a * (x**2) + b
plt.plot(x, y)
plt.xlabel("x")
plt.ylabel("f(x)")
plt.title(f"График функции f(x) = {a}x^2 + {b}")
plt.show()
